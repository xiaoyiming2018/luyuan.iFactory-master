@{
    Layout = null;
    @model List<Advantech.IFactory.CommonLibrary.unit_info>
}

<div class="col-sm-9">
    <select id="unit_id" name="unit_id" class="form-control" style="width:50%">
        <option value=""></option>
        @{
            foreach (var items in Model)
            {
                <option value="@items.seq">@items.unit_name_en</option>
            }
        }
    </select>
</div>

