@{
    Layout = null;
    @model List<Advantech.IFactory.CommonLibrary.line_info>
}

<div class="col-sm-9">
    <select id="line_id" name="line_id" class="form-control" style="width:50%">
        <option value=""></option>
        @{
            foreach (var items in Model)
            {
                <option value="@items.line_id">@items.line_name_en</option>
            }
        }
    </select>
</div>